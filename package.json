{
    "name":  "klusquest",
    "version":  "0.1.0",
    "private":  true,
  "scripts":  {
                    "dev":  "next dev",
                    "build":  "next build",
                    "start":  "next start",
                    "typecheck":  "tsc --noEmit",
                    "lint":  "next lint",
                    "prisma:generate":  "prisma generate",
                    "prisma:migrate":  "prisma migrate dev",
                    "db:seed":  "tsx prisma/seed.ts || true",
                    "test:e2e":  "playwright test",
                    "test:e2e:ui":  "playwright test --ui",
                    "test:e2e:headed":  "playwright test --headed",
                    "test:e2e:debug":  "playwright test --debug",
                    "test:e2e:report":  "playwright show-report",
                    "docs:check-encoding": "node scripts/check-encoding.mjs",
                    "docs:validate": "node scripts/validate-docs.mjs",
                    "docs:ci": "pnpm docs:check-encoding && pnpm docs:validate"
                },
    "dependencies":  {
                         "@clerk/nextjs":  "^6.31.4",
                         "@prisma/client":  "^5.7.0",
                         "next":  "^14.2.25",
                         "pixi.js":  "^7.3.0",
                         "react":  "^18",
                         "react-dom":  "^18",
                         "zod":  "^3.22.0"
                     },
    "prisma":  {
                   "seed":  "tsx prisma/seed.ts"
               },
    "devDependencies":  {
                            "@playwright/test":  "^1.55.0",
                            "@types/node":  "^20",
                            "@types/react":  "^18",
                            "@types/react-dom":  "^18",
                            "autoprefixer":  "^10.0.1",
                            "eslint":  "^8",
                            "eslint-config-next":  "14.0.0",
                            "postcss":  "^8",
                            "prisma":  "^5.7.0",
                            "tailwindcss":  "^3.3.0",
                            "tsx":  "^4.0.0",
                            "typescript":  "^5"
                        }
}
